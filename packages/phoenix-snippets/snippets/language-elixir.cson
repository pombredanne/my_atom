'.source.elixir':
  'pry':
    'prefix': 'pry'
    'body': 'IEx.pry'
'.text.elixir':
  'eex_render_block':
    'prefix': '='
    'body': '<%= $1 %>'
  'eex_exec_block':
    'prefix': '-'
    'body': '<% $1 %>'
  'eex_comment_block':
    'prefix': '#'
    'body': '<%# $1 %>'
  'eex_for':
    'prefix': 'for'
    'body': """
      <%= for ${1:model} <- @${1:model}s do %>
        $2
      <% end %>
    """
  'eex_error_tag':
    'prefix': 'et'
    'body': '<%= error_tag ${1:f}, :${1:field} %>'
  'eex_text_input':
    'prefix': 'ti'
    'body': '<%= text_input ${1:f}, :${1:field} %>'
  'eex_password_input':
    'prefix': 'pi'
    'body': '<%= password_input ${1:f}, :${1:field} %>'
  'eex_link':
    'prefix': 'lt'
    'body': """
      <%= link "${1:text}", to: ${2:url} %>
    """
  'eex_if':
    'prefix': 'if'
    'body': """
      <%= if $1 do %>
        $2
      <% end %>
    """
  'eex_if_else':
    'prefix': 'ife'
    'body': """
      <%= if $1 do %>
        $2
      <% else %>
        $3
      <% end %>
    """
  'eex_else':
    'prefix': 'else'
    'body': "<% else %>"
  'eex_unless':
    'prefix': 'unless'
    'body': """
      <%= unless $1 do %>
        $2
      <% end %>
    """
  'eex_cond':
    'prefix': 'cond'
    'body': """
      <%= cond do %>
        <% $1 -> %>
          $2
        <% true -> %>
          $3
      <% end %>
    """
  'eex_form_for':
    'prefix': 'ff'
    'body': """
      <%= form_for @${1:changeset}, ${2:url}, ${3:[]}, fn f -> %>
        $4
      <% end %>
    """
  'eex_render':
    'prefix': 'render'
    'body': """
      <%= render "${1:partial}.html", ${2:local_var: @local} %>
    """
